#include <stdio.h>

/* Exercise 1-3: Modify the temperature conversion program to print a heading
    above the table. */
int main(void)
{
    int low, high, step, CelTemp, FahTemp;
    
    FahTemp = low = 0;
    high = 300;
    step = 20;
    
    while (FahTemp <= high){
        CelTemp = 5 * (FahTemp - 32) / 9;
        FahTemp = FahTemp + step;
    }
    return 0;
}
